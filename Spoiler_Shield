t import TfidfVectorizer\nfrom tqdm import tqdm\nimport nltk\nfrom nltk.corpus import stopwords\nnltk.download('stopwords')","metadata":{"id":"tHJumJ_K34sL","trusted":true,"execution":{"iopub.status.busy":"2024-12-01T15:14:27.874966Z","iopub.execute_input":"2024-12-01T15:14:27.875429Z","iopub.status.idle":"2024-12-01T15:14:28.668556Z","shell.execute_reply.started":"2024-12-01T15:14:27.875390Z","shell.execute_reply":"2024-12-01T15:14:28.667294Z"}},"outputs":[{"name":"stderr","text":"[nltk_data] Downloading package stopwords to /usr/share/nltk_data...\n[nltk_data]   Package stopwords is already up-to-date!\n","output_type":"stream"},{"execution_count":19,"output_type":"execute_result","data":{"text/plain":"True"},"metadata":{}}],"execution_count":19},{"cell_type":"code","source":"def remove_stopwords(text):\n    stop_words = set(stopwords.words('english'))  # Use English stop words\n    words = text.lower().split()  # Tokenize and lowercase the text\n    filtered_words = [word for word in words if word.isalnum() and word not in stop_words]  # Filter out stop words and non-alphanumeric words\n    return ' '.join(filtered_words)  # Join the filtered words back into a string","metadata":{"trusted":true,"execution":{"iopub.status.busy":"2024-12-01T15:14:47.261645Z","iopub.execute_input":"2024-12-01T15:14:47.262102Z","iopub.status.idle":"2024-12-01T15:14:47.268783Z","shell.execute_reply.started":"2024-12-01T15:14:47.262047Z","shell.execute_reply":"2024-12-01T15:14:47.267296Z"}},"outputs":[],"execution_count":20},{"cell_type":"code","source":"def process_dataframe_in_batches(df, batch_size=1000, columns_to_process=None):\n    \"\"\"\n    Process DataFrame in batches\n    \n    Args:\n        df: Input DataFrame\n        batch_size: Number of rows to process at once\n        columns_to_process: List of column names to process (if None, process all text columns)\n    \"\"\"\n    # If no columns specified, process all object (string) columns\n    if columns_to_process is None:\n        columns_to_process = df.select_dtypes(include=['object']).columns\n    \n    # Create new column names for processed text\n    new_columns = {col: f\"{col}_processed\" for col in columns_to_process}\n    \n    # Initialize new columns with empty strings\n    for new_col in new_columns.values():\n        df[new_col] = ''\n    \n    # Process in batches\n    total_rows = len(df)\n           \n    for start_idx in tqdm(range(0, total_rows, batch_size), desc=\"Processing stop word removal\"):\n        end_idx = min(start_idx + batch_size, total_rows)\n        \n        print(f\"Processing rows {start_idx} to {end_idx}\")# Process each column in the batch\n        for col in columns_to_process:\n            # Get batch of text\n            batch_texts = df[col].iloc[start_idx:end_idx]\n            \n            # Apply stopword removal\n            processed_texts = batch_texts.apply(remove_stopwords)\n            \n            # Store results\n            df.loc[start_idx:end_idx, new_columns[col]] = processed_texts\n    \n    return df","metadata":{"trusted":true,"execution":{"iopub.status.busy":"2024-12-01T15:15:15.611122Z","iopub.execute_input":"2024-12-01T15:15:15.611586Z","iopub.status.idle":"2024-12-01T15:15:15.621040Z","shell.execute_reply.started":"2024-12-01T15:15:15.611548Z","shell.execute_reply":"2024-12-01T15:15:15.619693Z"}},"outputs":[],"execution_count":21},{"cell_type":"code","source":"def tfidf_batch_processing(\n    df: pd.DataFrame,\n    text_column: str,\n    batch_size: int = 5000,\n    max_features: int = 10000,\n    debug: bool = False\n) -> tuple:\n    try:\n        # First, verify the input data\n        if debug:\n            print(f\"Input DataFrame shape: {df.shape}\")\n            print(f\"Sample of text column:\\n{df[text_column].head()}\")\n\n        # Clean the text data first\n        df[text_column] = df[text_column].fillna('')\n        \n        # Verify there's actual text data\n        if df[text_column].str.strip().str.len().eq(0).all():\n            raise ValueError(\"No valid text data found\")\n\n        # Initialize vectorizer with more robust settings\n        vectorizer = TfidfVectorizer(\n            max_features=max_features,\n            min_df=1,                \n            max_df=0.99,            \n            strip_accents='unicode',\n            lowercase=True,\n            ngram_range=(1, 1),     \n            dtype=np.float32,\n            token_pattern=r'(?u)\\b\\w+\\b'  \n        )\n\n        try:\n            # Fit the vectorizer first\n            if debug:\n                print(\"Fitting vectorizer...\")\n            vectorizer.fit(df[text_column])\n            \n            if debug:\n                print(f\"Vocabulary size: {len(vectorizer.vocabulary_)}\")\n                \n            # Process in batches\n            total_rows = len(df)\n            tfidf_matrices = []\n            \n            for start_idx in tqdm(range(0, total_rows, batch_size)):\n                end_idx = min(start_idx + batch_size, total_rows)\n                batch_texts = df[text_column].iloc[start_idx:end_idx]\n                \n                # Transform batch\n                batch_tfidf = vectorizer.transform(batch_texts)\n                \n                tfidf_matrices.append(batch_tfidf)\n                \n                if debug:\n                    print(f\"Processed batch {start_idx}-{end_idx}\")\n\n            final_tfidf_matrix = sparse.vstack(tfidf_matrices)\n            \n            if debug:\n                print(f\"Final matrix shape: {final_tfidf_matrix.shape}\")\n            \n            return vectorizer, final_tfidf_matrix\n\n        except Exception as e:\n            print(f\"Error during vectorization: {str(e)}\")\n            if debug:\n                import traceback\n                traceback.print_exc()\n            return None, None\n\n    except Exception as e:\n        print(f\"Initialization error: {str(e)}\")\n        if debug:\n            import traceback\n            traceback.print_exc()\n        return None, None\n\n\n# Example usage:\ndef remove_stopwords_process_tfidf(df: pd.DataFrame) -> tuple:\n    \"\"\"\n    Process text data with TF-IDF vectorization\n    \n    Args:\n        df: DataFrame containing text data\n        \n    Returns:\n        tuple: (vectorizer, tfidf_matrix)\n    \"\"\"\n\n    # Input validation\n    if df is None or df.empty:\n        raise ValueError(\"Input DataFrame is empty or None\")\n        \n    print(\"Removing stopwords...\")\n     # Specify the columns to process\n    columns_to_process = ['review_text', 'plot_summary', 'plot_synopsis']\n    # Process the DataFrame in batches\n    batch_size = 10000  # Adjust based on your needs\n    processed_df = process_dataframe_in_batches(\n        df=df,\n        batch_size=batch_size,\n        columns_to_process=columns_to_process\n    )\n\n    # Combine all text columns if needed\n    if 'combined_text' not in processed_df.columns:\n        processed_df['combined_text'] = processed_df[['review_text_processed', \n                                'plot_summary_processed', \n                                'plot_synopsis_processed']].fillna('').agg(' '.join, axis=1)\n    \n    print(processed_df.info())\n    \n    print(\"Starting TF-IDF processing...\")\n    # Process TF-IDF in batches\n    vectorizer, tfidf_matrix = tfidf_batch_processing(\n        df=processed_df,\n        text_column='combined_text',\n        batch_size=5000,  # Adjust based on your memory constraints\n        max_features=10000,  # Adjust based on your needs\n        debug=True\n    )\n\n    if vectorizer is None:\n        raise ValueError(\"Vectorizer initialization failed\")\n        \n    # Verify matrix\n    if tfidf_matrix is None:\n        raise ValueError(\"Matrix creation failed\")\n        \n    # Verify dimensions\n    if tfidf_matrix.shape[0] != len(processed_df):\n        raise ValueError(f\"Matrix shape mismatch: {tfidf_matrix.shape[0]} vs {len(processed_df)}\")\n        \n    print(f\"Success! Matrix shape: {tfidf_matrix.shape}\")\n    \n    return vectorizer, tfidf_matrix\n\n# Example of how to use it:\n\"\"\"\n# First process your text data using the previous MPS batch processing\nprocessed_df = process_text_columns_with_monitoring(your_dataframe)\n\n# Then apply TF-IDF vectorization\nvectorizer, tfidf_matrix = process_text_with_tfidf(processed_df)\n\n# You can now use the TF-IDF matrix for further analysis\n# For example, to get feature names:\nfeature_names = vectorizer.get_feature_names_out()\n\n# To get the TF-IDF scores for a specific document:\ndocument_index = 0\ndocument_vector = tfidf_matrix[document_index]\n\"\"\"\n","metadata":{"trusted":true,"execution":{"iopub.status.busy":"2024-12-01T15:31:00.348231Z","iopub.execute_input":"2024-12-01T15:31:00.350112Z","iopub.status.idle":"2024-12-01T15:31:00.378816Z","shell.execute_reply.started":"2024-12-01T15:31:00.350035Z","shell.execute_reply":"2024-12-01T15:31:00.377436Z"}},"outputs":[{"execution_count":24,"output_type":"execute_result","data":{"text/plain":"'\\n# First process your text data using the previous MPS batch processing\\nprocessed_df = process_text_columns_with_monitoring(your_dataframe)\\n\\n# Then apply TF-IDF vectorization\\nvectorizer, tfidf_matrix = process_text_with_tfidf(processed_df)\\n\\n# You can now use the TF-IDF matrix for further analysis\\n# For example, to get feature names:\\nfeature_names = vectorizer.get_feature_names_out()\\n\\n# To get the TF-IDF scores for a specific document:\\ndocument_index = 0\\ndocument_vector = tfidf_matrix[document_index]\\n'"},"metadata":{}}],"execution_count":24},{"cell_type":"code","source":"vectorizer, tfidf_matrix = remove_stopwords_process_tfidf(spoiler_df)","metadata":{"trusted":true,"execution":{"iopub.status.busy":"2024-12-01T15:31:06.667558Z","iopub.execute_input":"2024-12-01T15:31:06.668009Z","iopub.status.idle":"2024-12-01T15:33:39.051159Z","shell.execute_reply.started":"202