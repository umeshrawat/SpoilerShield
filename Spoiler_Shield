ce(duration_str, str):\n        if 'h' in duration_str:\n            parts = duration_str.split('h')\n            hours = int(parts[0]) if parts[0] else 0\n            minutes = int(parts[1].split('min')[0]) if len(parts) > 1 and parts[1] else 0\n            total_minutes = hours * 60 + minutes\n        else:  # Handle strings with only minutes\n            total_minutes = int(duration_str.split('min')[0]) if 'min' in duration_str else 0 \n        return total_minutes\n    else:\n        return duration_str\n\n# Assuming your dataframe is named 'df' and the column is named 'duration'\ncombined_df['duration'] = combined_df['duration'].apply(duration_to_minutes)\ncombined_df['duration'] = combined_df['duration'].astype(float)","metadata":{"trusted":true,"execution":{"iopub.status.busy":"2024-11-25T20:26:00.925960Z","iopub.execute_input":"2024-11-25T20:26:00.926766Z","iopub.status.idle":"2024-11-25T20:26:01.495876Z","shell.execute_reply.started":"2024-11-25T20:26:00.926730Z","shell.execute_reply":"2024-11-25T20:26:01.495112Z"}},"outputs":[],"execution_count":null},{"cell_type":"code","source":"# Attempt to convert dates with errors='coerce'\ncombined_df['release_date'] = pd.to_datetime(combined_df['release_date'],errors='coerce').astype(int) / 10**9 \nprint(combined_df['release_date'].value_counts())","metadata":{"trusted":true,"execution":{"iopub.status.busy":"2024-11-25T20:26:08.415716Z","iopub.execute_input":"2024-11-25T20:26:08.416413Z","iopub.status.idle":"2024-11-25T20:26:08.539097Z","shell.execute_reply.started":"2024-11-25T20:26:08.416382Z","shell.execute_reply":"2024-11-25T20:26:08.538209Z"}},"outputs":[],"execution_count":null},{"cell_type":"code","source":"combined_df['review_date'] = pd.to_datetime(combined_df['review_date']).astype(int) / 10**9 \n\n# This converts to Unix timestamps in seconds","metadata":{"trusted":true,"execution":{"iopub.status.busy":"2024-11-25T20:26:20.988940Z","iopub.execute_input":"2024-11-25T20:26:20.990027Z","iopub.status.idle":"2